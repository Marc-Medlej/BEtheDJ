<div class="container bg" style="background-image: url('data:image/jpeg;base64,')">

  <div class="btn add-new link-whiten mt-4">
    <%= link_to "Add a new event for #{@venue.name}", new_venue_event_path(@venue) %>
  </div>

  <h1 class="text-white text-center"><%= @venue.name %></h1>
  <div class="container">
    <div class="row">
      <% @events.each do |event| %>
        <div class="col-xs-12 col-sm-12 col-md-6 col-lg-6 col-xl-6">
          <div class="card-product flex-container space-between m-3 p-3">
            <%= link_to "", event_path(event), class: "card-link"%>
            <%= image_tag event.photo %>
              <!--   # link_to "", event_path(event), class: "card-link"
                # if event.photo.attached?
                  # cl_image_tag event.photo.key, height: 300, width: 400, crop: :fill
                 end  -->
              <div class="card-product-infos mt-3">
                <h4><%= event.name %></h4>
                <p><i class="fas fa-user-alt"></i> DJ: </p>
                <p><i class="fas fa-music"></i></i> Genre: </p>
              </div>
              <div class="card-date">
                <p><i class="fas fa-calendar-alt"></i> <%= event.date.strftime("%d/%m/%Y") %></p>
              </div>
          </div>
        </div>
      <% end %>
    </div>
</div>
